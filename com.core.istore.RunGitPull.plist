<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.core.istore.RunGitPull.plist</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/bin/git</string>
		<string>-C</string>
		<string>/Library/iStores</string>
		<string>pull</string>
	</array>
	<array>
		<string>/Library/iStore/Script</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StartInterval</key>
	<integer>20</integer>
</dict>
</plist>
